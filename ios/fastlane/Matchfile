# Use environment variable for GitHub token
# Set GITHUB_TOKEN environment variable before running fastlane
git_url(ENV["MATCH_FILE_GITHUB_URL"])

storage_mode("git")

type("appstore") # The default type, can be: appstore, adhoc, enterprise or development

# Specify all bundle identifiers for your targets
app_identifier([
  "com.5vnetwork.x",                    # Main app
  "com.5vnetwork.x.PacketTunnel",       # PacketTunnel extension
  "com.5vnetwork.x.ImageNotification"   # ImageNotification extension
])


# For all available options run `fastlane match --help`
# Remove the # in the beginning of the line to enable the other options

# The docs are available on https://docs.fastlane.tools/actions/match
